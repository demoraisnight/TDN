package ex5;

import java.util.HashMap;

public class Troco {
	
	

	public static void main(String[] args) {
		
	HashMap<String, Float> map = new HashMap<>();


    map.put("nota de 100", 100);
    map.put("nota de 50", 50);
    map.put("nota de 20", 20);
    map.put("nota de 10", 10);
    map.put("nota de 5", 5);
    map.put("nota de 2", 2);
    map.put("nota de 1", 1);
    
    map.put("moeda de 50", 0.5);
    map.put("nota de 20", 0.2);
    map.put("nota de 10", 0.1);
    map.put("nota de 5", 0.05);
    map.put("nota de 1", 0.01);
    
    for (String chave : map.keySet()) {
        float valor = map.get(chave);
        System.out.println("Chave: " + chave + ", Valor: " + valor);
    }
    
    
	}
}
